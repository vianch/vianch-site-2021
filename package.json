  "scripts": {
    "build": "APP_ENV=production gatsby build",
    "dev": "APP_ENV=development gatsby develop",
    "start": "APP_ENV=production gatsby serve -H 0.0.0.0 -p 8080",
    "lint": "eslint . --ext .ts,.tsx",
